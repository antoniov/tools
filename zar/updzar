# 0.2.6
srcdir=/opt/odoo/dev/pypi/zar/zar
tgtdir=/root
for f in bckconf bckconf.py restconf restconf.py bckwww restwww bckdb bckdb.py restdb restdb.py; do
  echo "Copying $srcdir/$f $tgtdir/$f"
  cp $srcdir/$f $tgtdir/$f
done
c=$(readlink -e $0)
if [ "$c" == "$srcdir/updzar" ]; then
  f=updzar
  echo "Copying $srcdir/$f $tgtdir/$f"
  cp $srcdir/$f $tgtdir/$f
fi
