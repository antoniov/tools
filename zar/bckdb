#
THIS=$(basename $0)
TDIR=$(readlink -f $(dirname $0))
for x in $TDIR $TDIR/.. . .. $TDIR/../../z0lib /etc; do
  if [ -e $x/z0librc ]; then
    . $x/z0librc
    Z0LIBDIR=$x
    Z0LIBDIR=$(readlink -e $Z0LIBDIR)
    break
  fi
done
if [ -z "$Z0LIBDIR" ]; then
  echo "Library file z0librc not found!"
  exit 2
fi

__version__=2.1.15


LogResult () {
    publog="/var/log/nightly.log"
    set_tlog_file "$publog"
    wlog "- $THIS - $1"
}

#python ${0}.py
python bckdb.py
LogResult "Ok!"
exit 0
